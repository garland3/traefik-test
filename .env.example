# OAuth Provider Configuration
OAUTH_PROVIDER=azure  # or 'auth0'
CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here
AUTHORITY=your_authority_here  # For Azure: tenant_id, for Auth0: your_domain.auth0.com
REDIRECT_URI=http://localhost/getAToken
SCOPE=openid profile email  # Space-separated list of scopes

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here 